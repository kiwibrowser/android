{
  "manifest_version": 2,
  "name": "NTP content script injection",
  "version": "1",
  "description": "Test for crbug.com/844428",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content_script.js"
      ],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
